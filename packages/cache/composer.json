{
    "name": "windwalker/cache",
    "type": "windwalker-package",
    "description": "Windwalker Cache package",
    "keywords": [
        "windwalker",
        "framework",
        "cache",
        "psr-6",
        "psr-16"
    ],
    "homepage": "https://github.com/ventoviro/windwalker-event",
    "license": "MIT",
    "require": {
        "php": ">=8.0.0",
        "psr/cache": "^1.0||^2.0||^3.0",
        "psr/simple-cache": "^1.0",
        "psr/log": "^1.0",
        "windwalker/utilities": "4.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0||^9.0",
        "windwalker/test": "4.x-dev",
        "windwalker/filesystem": "4.x-dev",
        "league/flysystem": "^2.0",
        "mockery/mockery": "^1.4"
    },
    "suggest": {
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Windwalker\\Cache\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Cache\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-4": "4.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        }
    },
    "provide": {
        "psr/cache-implementation": "~1.0",
        "psr/simple-cache-implementation": "~1.0"
    }
}
